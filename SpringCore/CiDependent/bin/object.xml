<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
 
 <bean id="a" class="com.techment.modal.Address">
 <constructor-arg value="Durg"></constructor-arg>
 <constructor-arg value="Chhattisgarh"></constructor-arg>
 <constructor-arg value="India"></constructor-arg>
 </bean>
 
 <bean id="e" class="com.techment.modal.Employee">
<constructor-arg value="1"></constructor-arg>

<constructor-arg value="Hemashree"></constructor-arg>
 
<constructor-arg ><ref bean="a"/></constructor-arg>
 
 
 
 </bean>
  </beans>